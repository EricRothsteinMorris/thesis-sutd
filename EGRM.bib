	Issn = {00104620},
	Journal = {Computer Journal},
	Keywords = {ALGEBRA, COMPUTER science, MODALITY (Logic), LOGIC, SEMANTICS, MACHINE theory, CONCURRENT engineering, automata theory, coalgebra, compositionality, concurrency, knowledge representation, modal logic},
	Number = {1},
	Pages = {31 - 41},
